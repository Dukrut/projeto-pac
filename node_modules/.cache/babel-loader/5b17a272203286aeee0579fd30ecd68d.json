ACA,eAAA,KAAA,CAAA,GAAA,IAAA,IAAA;AACA,UAAA,KAAA,GAAA,KAAA;AACA;AACA;;AACA,aAAA,KAAA;AACA,KAxBA;AAyBA,IAAA,KAAA,EAAA,iBAAA;AACA,UAAA,CAAA,KAAA,WAAA,CAAA,KAAA,IAAA,CAAA,EAAA;AACA,aAAA,MAAA,CAAA,4CAAA,EAAA,OAAA;;AACA,eAAA,KAAA;AACA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA;AA/BA;AAdA,CAAA","sourcesContent":["<template>\n  <div class=\"body\">\n\n    <v-card\n      elevation=\"10\"\n      rounded=\"xl\"\n      max-width=\"25%\"\n      max-height=\"50%\"\n      color=\"#D5EAFF\"\n      align-self=\"center\"\n      class=\"card-login\"\n      min-width=\"250px\"\n      min-height=\"350px\"\n    >\n    <v-row\n      class=\"justify-center\"\n    >\n    <h2 class=\"mt-5 mb-5 text-truncate\">Seja Bem-Vindo!</h2>\n    </v-row>\n    <v-col\n      cols=\"12\"\n      align-self=\"center\"\n      class=\"d-flex justify-center\"\n    >\n      <v-col\n        cols=\"10\"\n      >\n        <v-text-field\n          outlined\n          label=\"Usuário\"\n          prepend-inner-icon=\"mdi-account\"\n          :error=error.login\n          clearable\n          rounded\n          @input=\"clearError('login')\"\n        ></v-text-field>\n        <v-text-field\n          outlined\n          label=\"Senha\"\n          prepend-inner-icon=\"mdi-lock\"\n          :error=error.password\n          type=\"password\"\n          @input=\"clearError('password')\"\n          rounded\n        ></v-text-field>\n      </v-col>\n    </v-col>\n\n    <v-row justify=\"space-around\">\n      <v-btn\n        rounded\n        color=\"primary\"\n        class=\"mb-5\"\n        :loading=loading\n        @click=\"login()\"\n      >\n      Entrar\n      </v-btn>\n      <v-btn\n        rounded\n        color=\"primary\"\n        class=\"mb-5\"\n        :disabled=loading\n      >\n      Inscreva-se!\n      </v-btn>\n    </v-row>\n  </v-card>\n\n  </div>\n</template>\n\n<script>\nexport default {\n  data: () => ({\n    form: {\n      login: \"\",\n      password: \"\"\n    },\n    error: {\n      login: false,\n      password: false,  \n    },\n    loading: false,\n  }),\n  mounted() {\n  },\n  methods: {\n    _toast: function(_message, _type){\n      this.$toast.open({\n        message: _message,\n        type: _type,\n        position: \"top-right\",\n      });\n    },\n    regexTypes: function() {\n      return [null, undefined, \"\"]\n    },\n    clearError: function(prop){\n      this.error[prop] = false;\n    },\n    validInputs: function(form) {\n      var valid = true;\n      for (const key in form) {\n        const element = form[key];\n        if (this.regexTypes().includes(element)){\n          this.error[key] = true;          \n          valid = false\n        }\n      }\n      return valid;\n    },\n    login: function() {\n      if (!this.validInputs(this.form)) {\n        this._toast('O campo Usuário e Senha são obrigatórios.\"', 'error')\n        return false;\n      }\n      this.loading = true\n    }\n  }\n}\n</script>\n\n<style>\n  .card-login{\n    margin-top:15%;\n    margin-left:15%;\n  }\n</style>\n"],"sourceRoot":"src/components"}]}